						</div>
					</div>
					<div class="carousel-item" style="background-image:url(/images/images/blog-mobile-bg.png)">
						<div class="container">
							<h1 class="banner_taital">Cua Cá's Flower'</h1>
							<p class="banner_text">Chương trình khởi nghiệp của bạn Cá </p>
							<div class="read_bt"><a href="#">Get A Quote</a></div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- banner section end -->
	</div>
	<!-- header section end -->
	<div class="center_content">
		@RenderBody()
	</div>
	<!-- footer section start -->
	<div class="footer_section layout_padding">
		<div class="container" style="background-image:url(/images/images/blog-mobile-bg.png)">
			<div class="input_btn_main">
				<input type="text" class="mail_text" placeholder="Enter your email" name="Enter your email">
				<div class="subscribe_bt"><a href="#">Subscribe</a></div>
			</div>
			<div class="location_main">
				<div class="call_text"><img src="~/images/images/call-icon.png"></div>
				<div class="call_text"><a href="#">Call 0865998603</a></div>
				<div class="call_text"><img src="~/images/images/mail-icon.png"></div>
				<div class="call_text"><a href="#">handtph26146@fpt.edu.vn</a></div>
			</div>
			<div class="social_icon">
				<ul>
					<li><a href="#"><img src="~/images/images/fb-icon.png"></a></li>
					<li><a href="#"><img src="~/images/images/twitter-icon.png"></a></li>
					<li><a href="#"><img src="~/images/images/linkedin-icon.png"></a></li>
					<li><a href="#"><img src="~/images/images/instagram-icon.png"></a></li>
				</ul>
			</div>
		</div>
	</div>
	<!-- footer section end -->
	<!-- copyright section start -->
	<div class="copyright_section">
		<div class="container">
			<p class="copyright_text">2020 All Rights Reserved. Design by <a href="https://html.design">Free html  Templates</a></p>
		</div>
